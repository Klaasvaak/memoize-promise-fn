{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aldrinpvincent/promise-memoize"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "memoize-promise-fn",
  "author": "aldrinpvincent",
  "description": "A utility to cache api calls in JavaScript by memoizing promises",
  "module": "dist/memoize-promise-fn.esm.js",
  "size-limit": [
    {
      "path": "dist/memoize-promise-fn.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/memoize-promise-fn.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "husky": "^7.0.4",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2"
  },
  "keywords": [
    "javascript",
    "caching",
    "promise",
    "memoization",
    "performance"
  ]
}